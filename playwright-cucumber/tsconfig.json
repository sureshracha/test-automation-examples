{
    "compilerOptions": {
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "target": "ES2021",
        "moduleResolution": "Node",
        "module": "CommonJS",
        "declaration": true,
        "isolatedModules": true,
        "noEmit": true,
        "outDir": "playwright-cucumber/dist"
    }, 
    "include": ["playwright-cucumber/src"],
    "exclude": ["playwright-cucumber/node_modules"]
}